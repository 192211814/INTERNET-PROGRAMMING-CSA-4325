<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Fetch Data with JavaScript</title>
</head>
<body>
    <h1>User Data</h1>
    <div id="userData"></div>
    <script>
    function fetchData() {
fetch('https://jsonplaceholder.typicode.com/users')
.then(response => response.json())
.then(data => {
 let userData = document.getElementById('userData');
 userData.innerHTML = '<h2>Users</h2>';
 data.forEach(user => {
 userData.innerHTML += <p><strong>Name:</strong> ${user.name}, <strong>Email:</strong> ${user.email}</p>;
 });
 })
.catch(error => {
console.error('Error fetching data:', error);
});
}
window.onload = fetchData;
</script>
</body>
</html>
